{
  "name": "mutiny-theme",
  "version": "1.0.0",
  "description": "Theme",
  "main": "npm run production",
  "license": "GPLv3",
  "private": true,
  "homepage": "https://mutiny.pl",
  "repository": {
    "type": "git",
    "url": "https://github.com/MutinyGroup/theme.git"
  },
  "bugs": {
    "url": "https://github.com/MutinyGroup/theme/issues"
  },
  "contributors": [
    {
      "name": "Patryk Padus",
      "email": "patryk.padus@webo.agency",
      "url": "https://www.webo.agency"
    },
    {
      "name": "Maciej Zawadzki",
      "email": "maciej.zawadzki@webo.agency",
      "url": "https://www.webo.agency"
    }
  ],
  "engines": {
    "node": ">=v16.11.1",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "cf_token": "bash ./cf_token.sh 'https://test.shop-mancraft.com' true",
    "serve": "cross-env NODE_ENV=development TEST_URL=https://test.shop-mancraft.com concurrently \"postcss ./css/index.css -o assets/css/styles.min.css --watch\"  \"npx browser-sync start --config bs-config.js\"",
    "test": "npx stylelint './assets/**/*.css'",
    "stylelint-check": "stylelint-config-prettier-check",
    "development": "cross-env NODE_ENV=development postcss ./assets/custom-styles.css -o assets/styles.min.css",
    "production": "cross-env NODE_ENV=production postcss ./assets/custom-styles.css -o assets/styles.min.css",
    "blank": "echo \"Error: no test specified\"",
    "deploy": "npm run production"
  },
  "dependencies": {
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.4",
    "@tailwindcss/line-clamp": "^0.4.4",
    "@tailwindcss/typography": "^0.5.9",
    "autoprefixer": "^10.4.15",
    "browser-sync": "2.29.3",
    "concurrently": "^8.2.0",
    "cross-env": "^7.0.3",
    "cssnano": "^6.0.1",
    "dotenv": "^16.3.1",
    "postcss": "^8.4.28",
    "postcss-cli": "^10.1.0",
    "postcss-discard-empty": "^6.0.0",
    "postcss-import": "^15.1.0",
    "postcss-nesting": "^12.0.1",
    "postcss-reporter": "^7.0.5",
    "tailwindcss": "^3.3.3",
    "tailwindcss-blend-mode": "^1.0.0",
    "tailwindcss-gradients": "^3.0.0"
  },
  "devDependencies": {
    "prettier": "^3.0.2",
    "prettier-plugin-tailwindcss": "^0.5.3",
    "prettier-stylelint": "^0.4.2",
    "stylelint": "^15.10.2",
    "stylelint-config-standard": "^34.0.0"
  }
}
